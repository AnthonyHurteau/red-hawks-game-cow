{
    "configurations": [
        {
            "name": "startApp",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}/app",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "startApi",
            "type": "node",
            "request": "launch",
            "preLaunchTask": "runApi",
        },
    ],
    "compounds": [
        {
            "name": "Full Stack",
            "configurations": [
                "startApp",
                "startApi",
            ]
        }
    ]
}